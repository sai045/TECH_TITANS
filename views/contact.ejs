    overflow: hidden;
    font-size: 0.8rem;
  }

  .menudrop:hover {
    background-color: #F3C693;
  }


  .menudrop .menubtn {
    font-size: 16px;
    border: none;
    outline: none;
    color: black;
    padding: 8px 16px;
    background-color: #F3C693;
    font-family: inherit;
    margin: 10px 100px;
    border-radius: 10px;
  }

  .menu-content {
    display: none;
    position: absolute;
    background-color: #f9f9f9;
    min-width: 160px;
    box-shadow: 0px 8px 16px 0px rgba(0, 0, 0, 0.2);
    z-index: 1;

  }

  .menu-content a {
    float: none;
    color: black;
    padding: 12px 16px;
    text-decoration: none;
    display: block;
    text-align: left;
  }

  .menu-content a:hover {
    background-color: #ddd;
  }

  .menudrop:hover .menu-content {
    display: block;
  }
</style>

