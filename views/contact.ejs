<style>
  body {
    background-image: url('https://images4.alphacoders.com/864/thumb-1920-864464.png');
    background-repeat: no-repeat;
    background-size: cover;
  }

  * {
    margin: 0;
    padding: 0;
  }

  #navbar {
    position: sticky;
    top: 0px;

  }

  #navbar ul {
    display: flex;
    list-style: none;
    justify-content: end;


  }

  #navbar ul li {
    margin: 15px 25px;
    font-size: 1.5rem;
    border-radius: 10px;
    color: white;
  }

  #navbar::before {
    content: "";
    background-color: black;
    position: absolute;
    top: 0px;
    left: 0px;
    height: 100%;
    width: 100%;
    z-index: -1;
    opacity: 0.5;
    padding: 0px;
  }

  #navbar ul li button {
    padding: 10px;
    color: black;
    border-radius: 16px;

  }

  * {
    box-sizing: border-box;
  }

  body {
    font-family: poppins;
    font-size: 16px;
    color: rgb(0, 4, 5);
  }

  .formbox .heading {
    font-size: 32px;
    font-weight: 600;
    padding-bottom: 10px;
    text-align: center;
    margin: -41px 0px 10px 820px;
    width: 30%;

  }

  .formbox input {
    background: rgb(230, 196, 235);
    opacity: 0.8;
    margin: 10px 0px 25px 830px;
    padding: 10px 60px 10px 10px;
    border-radius: 5px;
    width: 30%;
    font-size: 18px;
    font-family: poppins;

  }


  .formbox label {
    position: relative;
    margin-left: 5px;
    margin-right: 10px;
    top: 5px;
    display: inline-block;
    width: 20px;
    height: 20px;
    cursor: pointer;


  }

  .formbox label:before {
    content: "";
    display: inline-block;
    width: 20px;
    height: 20px;
    border-radius: 5px;
    position: absolute;
    left: 0;
    bottom: 1px;

  }

  /*.formbox input[type=checkbox]:checked+label:before {
