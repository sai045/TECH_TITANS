	line-height: 20px;
}*/
  .formbox span {
    font-size: 14px;
  }

  .formbox button {
    background-color: rgb(201, 60, 170);
    color: black;
    border: none;
    border-radius: 5px;
    cursor: pointer;
    width: 10%;
    font-size: 18px;
    padding: 10px;
    margin: 0px 0px 0px 970px;
  }

  .formbox button:hover {
    background-color: rgb(24, 25, 27);
    color: whitesmoke;
  }

  .formbox textarea {
    background: rgb(230, 196, 235);
    opacity: 0.8;
    margin: 10px 0px 25px 830px;
    border: none;
    padding: 10px 60px 10px 10px;
    border-radius: 5px;
    width: 30%;
    font-size: 18px;
    font-family: poppins;
  }

